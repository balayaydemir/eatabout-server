{
  "name": "eatabout-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
    "seed": "psql -U eatabout-admin -d eatabout -f ./seeds/seed.eatabout_tables.sql",
    "seed:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run seed",
    "seed:test": "psql -U eatabout-admin -d eatabout-test -f ./seeds/seed.eatabout_tables.sql",
    "start": "node src/server.js",
    "predeploy": "npm audit && npm run migrate:production && npm run seed:production",
    "deploy": "git push heroku master"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.17.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-form-data": "^2.0.10",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "pg": "^7.12.1",
    "treeize": "^2.1.2",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "postgrator-cli": "^3.2.0",
    "supertest": "^4.0.2"
  }
}
